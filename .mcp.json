{
  "mcpServers": {
    "langgraph-agent": {
      "command": "uv",
      "args": [
        "run",
        "--frozen",
        "python",
        "-m",
        "mcp_server_langgraph.mcp.server_stdio"
      ],
      "env": {
        "PYTHONPATH": "./src"
      },
      "envFile": "./.env"
    },
    "langgraph-agent-streamable": {
      "command": "uv",
      "args": [
        "run",
        "--frozen",
        "python",
        "-m",
        "mcp_server_langgraph.mcp.server_streamable"
      ],
      "env": {
        "PYTHONPATH": "./src",
        "HOST": "0.0.0.0",
        "PORT": "8000"
      },
      "envFile": "./.env"
    }
  }
}
