        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
        "id": 8,
        "options": { "legend": { "calcs": ["last", "sum"], "displayMode": "table", "placement": "bottom", "showLegend": true }, "tooltip": { "mode": "multi", "sort": "none" } },
        "pluginVersion": "10.0.0",
        "targets": [{ "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(builder_validation_total[5m])) by (result)", "legendFormat": "{{result}}", "refId": "A" }],
