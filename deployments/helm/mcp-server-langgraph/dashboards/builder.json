      "options": { "legend": { "calcs": ["last", "sum"], "displayMode": "table", "placement": "bottom", "showLegend": true }, "tooltip": { "mode": "multi", "sort": "none" } },
      "pluginVersion": "10.0.0",
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(builder_code_generation_total{status=\"success\"}[5m]))", "legendFormat": "Success", "refId": "A" },
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(builder_code_generation_total{status=\"error\"}[5m]))", "legendFormat": "Error", "refId": "B" }
      ],
      "title": "Code Generation Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Code generation duration percentiles (p50, p95, p99).",
      "fieldConfig": {
        "defaults": {
