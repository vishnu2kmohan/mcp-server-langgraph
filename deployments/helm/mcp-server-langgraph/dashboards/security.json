    { "collapsed": false, "gridPos": { "h": 1, "w": 24, "x": 0, "y": 4 }, "id": 100, "panels": [], "title": "Security Trends", "type": "row" },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Authentication failures over time broken down by reason. Identify attack patterns and failure types.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
