      "tooltip": "View Redis Sessions Dashboard",
      "type": "link",
      "url": "/d/redis-sessions/redis-sessions"
    },
    {
      "asDropdown": false,
      "icon": "doc",
      "includeVars": true,
      "keepTime": true,
      "tags": [],
      "targetBlank": true,
      "title": "ðŸ“– Runbook",
      "tooltip": "View Authentication Alerts Runbook",
      "type": "link",
      "url": "https://github.com/vishnu2kmohan/mcp-server-langgraph/blob/main/monitoring/runbooks/authentication-alerts.md"
    }
  ],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "description": "Current auth service health. GREEN = UP, RED = DOWN. Check immediately if RED.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "options": {
                "0": { "color": "red", "index": 0, "text": "DOWN" },
                "1": { "color": "green", "index": 1, "text": "UP" }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          },
          "unit": "none"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "up{job=\"langgraph-agent\"}",
          "refId": "A"
        }
      ],
      "title": "Status",
      "type": "gauge"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Current login attempt rate per second. Monitor for traffic patterns and spikes.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 200 }
            ]
          },
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 },
      "id": 2,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(rate(auth_login_attempts_total[5m]))",
          "refId": "A"
        }
      ],
      "title": "Login Rate",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Percentage of failed logins. YELLOW > 1%, RED > 5%. Investigate if elevated.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.05 }
            ]
          },
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 },
      "id": 3,
      "options": {
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(rate(auth_login_failures_total[5m])) / (sum(rate(auth_login_attempts_total[5m])) + 0.0001)",
          "refId": "A"
        }
      ],
      "title": "Failure Rate",
      "type": "gauge"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "95th percentile login latency. YELLOW > 500ms, RED > 1s. Slow logins impact UX.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "red", "value": 1000 }
            ]
          },
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 },
      "id": 4,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "histogram_quantile(0.95, sum(rate(auth_login_duration_seconds_bucket[5m])) by (le)) * 1000",
          "refId": "A"
        }
      ],
      "title": "p95 Latency",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Current number of active sessions. YELLOW > 1000, RED > 5000. Monitor for session leaks.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1000 },
              { "color": "red", "value": 5000 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
      "id": 5,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(auth_sessions_active)",
          "refId": "A"
        }
      ],
      "title": "Active Sessions",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "JWKS cache hit rate. Low values (<90%) indicate excessive key fetching. Check Keycloak connectivity.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.8 },
              { "color": "green", "value": 0.95 }
            ]
          },
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
      "id": 6,
      "options": {
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(rate(auth_jwks_cache_operations_total{type=\"hit\"}[5m])) / (sum(rate(auth_jwks_cache_operations_total{type=\"hit\"}[5m])) + sum(rate(auth_jwks_cache_operations_total{type=\"miss\"}[5m])) + 0.0001)",
          "refId": "A"
        }
      ],
      "title": "JWKS Cache",
      "type": "gauge"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 4 },
      "id": 100,
      "panels": [],
      "title": "Login Activity",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Login activity over time showing attempts, successes, and failures. Identify patterns and spikes.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false, "axisColorMode": "text", "axisLabel": "", "axisPlacement": "auto",
            "barAlignment": 0, "drawStyle": "line", "fillOpacity": 10, "gradientMode": "none",
            "hideFrom": { "tooltip": false, "viz": false, "legend": false },
            "lineInterpolation": "linear", "lineWidth": 1, "pointSize": 5,
            "scaleDistribution": { "type": "linear" }, "showPoints": "never", "spanNulls": true,
            "stacking": { "group": "A", "mode": "none" }, "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
          "unit": "reqps"
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Attempts" }, "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Successful" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Failed" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 5 },
      "id": 7,
      "options": {
        "legend": { "calcs": ["last", "max", "mean"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "pluginVersion": "10.0.0",
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(auth_login_attempts_total[5m]))", "legendFormat": "Attempts", "refId": "A" },
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(auth_login_attempts_total[5m])) - sum(rate(auth_login_failures_total[5m]))", "legendFormat": "Successful", "refId": "B" },
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(auth_login_failures_total[5m]))", "legendFormat": "Failed", "refId": "C" }
      ],
      "title": "Login Activity Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Login latency percentiles (p50, p95, p99). Identify slow authentication and performance degradation.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false, "axisColorMode": "text", "axisLabel": "", "axisPlacement": "auto",
            "barAlignment": 0, "drawStyle": "line", "fillOpacity": 10, "gradientMode": "none",
            "hideFrom": { "tooltip": false, "viz": false, "legend": false },
            "lineInterpolation": "linear", "lineWidth": 1, "pointSize": 5,
            "scaleDistribution": { "type": "linear" }, "showPoints": "never", "spanNulls": true,
            "stacking": { "group": "A", "mode": "none" }, "thresholdsStyle": { "mode": "line" }
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 500 }, { "color": "red", "value": 1000 }] },
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 5 },
      "id": 8,
      "options": {
        "legend": { "calcs": ["mean", "max", "last"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "pluginVersion": "10.0.0",
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "histogram_quantile(0.50, sum(rate(auth_login_duration_seconds_bucket[5m])) by (le)) * 1000", "legendFormat": "p50", "refId": "A" },
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "histogram_quantile(0.95, sum(rate(auth_login_duration_seconds_bucket[5m])) by (le)) * 1000", "legendFormat": "p95", "refId": "B" },
        { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "histogram_quantile(0.99, sum(rate(auth_login_duration_seconds_bucket[5m])) by (le)) * 1000", "legendFormat": "p99", "refId": "C" }
      ],
      "title": "Login Response Time Percentiles",
      "type": "timeseries"
    },
    {
      "collapsed": true,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "id": 101,
      "panels": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "description": "Token operation rates: created, verified, refreshed, and verification failures.",
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": {
                "axisCenteredZero": false, "axisColorMode": "text", "axisLabel": "", "axisPlacement": "auto",
                "barAlignment": 0, "drawStyle": "line", "fillOpacity": 10, "gradientMode": "none",
                "hideFrom": { "tooltip": false, "viz": false, "legend": false },
                "lineInterpolation": "linear", "lineWidth": 1, "pointSize": 5,
                "scaleDistribution": { "type": "linear" }, "showPoints": "never", "spanNulls": true,
                "stacking": { "group": "A", "mode": "none" }, "thresholdsStyle": { "mode": "off" }
              },
              "mappings": [],
              "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
              "unit": "reqps"
            },
            "overrides": []
          },
          "gridPos": { "h": 8, "w": 8, "x": 0, "y": 14 },
          "id": 9,
          "options": {
            "legend": { "calcs": ["last", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
            "tooltip": { "mode": "multi", "sort": "none" }
          },
          "pluginVersion": "10.0.0",
          "targets": [
            { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(auth_token_created_total[5m]))", "legendFormat": "Created", "refId": "A" },
            { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(auth_token_verifications_total{result=\"success\"}[5m]))", "legendFormat": "Verified", "refId": "B" },
            { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(auth_token_refresh_total{result=\"success\"}[5m]))", "legendFormat": "Refreshed", "refId": "C" },
            { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(auth_token_verifications_total{result!=\"success\"}[5m]))", "legendFormat": "Verify Failed", "refId": "D" }
          ],
          "title": "Token Operations Rate",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "description": "Token verification results breakdown by status (success, expired, invalid, revoked).",
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": {
                "axisCenteredZero": false, "axisColorMode": "text", "axisLabel": "", "axisPlacement": "auto",
                "barAlignment": 0, "drawStyle": "line", "fillOpacity": 10, "gradientMode": "none",
                "hideFrom": { "tooltip": false, "viz": false, "legend": false },
                "lineInterpolation": "linear", "lineWidth": 1, "pointSize": 5,
                "scaleDistribution": { "type": "linear" }, "showPoints": "never", "spanNulls": true,
                "stacking": { "group": "A", "mode": "none" }, "thresholdsStyle": { "mode": "off" }
              },
              "mappings": [],
              "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
              "unit": "reqps"
            },
            "overrides": []
          },
          "gridPos": { "h": 8, "w": 8, "x": 8, "y": 14 },
          "id": 10,
          "options": {
            "legend": { "calcs": ["last"], "displayMode": "table", "placement": "bottom", "showLegend": true },
            "tooltip": { "mode": "multi", "sort": "none" }
          },
          "pluginVersion": "10.0.0",
          "targets": [
            { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(auth_token_verifications_total[5m])) by (result)", "legendFormat": "{{result}}", "refId": "A" }
          ],
          "title": "Token Verification by Result",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "description": "Login failure breakdown by reason (invalid_credentials, expired, locked, etc).",
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": {
                "axisCenteredZero": false, "axisColorMode": "text", "axisLabel": "", "axisPlacement": "auto",
                "barAlignment": 0, "drawStyle": "line", "fillOpacity": 10, "gradientMode": "none",
                "hideFrom": { "tooltip": false, "viz": false, "legend": false },
                "lineInterpolation": "linear", "lineWidth": 1, "pointSize": 5,
                "scaleDistribution": { "type": "linear" }, "showPoints": "never", "spanNulls": true,
                "stacking": { "group": "A", "mode": "none" }, "thresholdsStyle": { "mode": "off" }
              },
              "mappings": [],
              "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
              "unit": "reqps"
            },
            "overrides": []
          },
          "gridPos": { "h": 8, "w": 8, "x": 16, "y": 14 },
          "id": 11,
          "options": {
            "legend": { "calcs": ["last"], "displayMode": "table", "placement": "bottom", "showLegend": true },
            "tooltip": { "mode": "multi", "sort": "none" }
          },
          "pluginVersion": "10.0.0",
          "targets": [
            { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(auth_login_failures_total[5m])) by (reason)", "legendFormat": "{{reason}}", "refId": "A" }
          ],
          "title": "Login Failures by Reason",
          "type": "timeseries"
        }
      ],
      "title": "Token Management",
      "type": "row"
    },
    {
      "collapsed": true,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 102,
      "panels": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "description": "Session lifecycle operations: created, revoked by backend (redis, memory, postgres).",
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": {
                "axisCenteredZero": false, "axisColorMode": "text", "axisLabel": "", "axisPlacement": "auto",
                "barAlignment": 0, "drawStyle": "line", "fillOpacity": 10, "gradientMode": "none",
                "hideFrom": { "tooltip": false, "viz": false, "legend": false },
                "lineInterpolation": "linear", "lineWidth": 1, "pointSize": 5,
                "scaleDistribution": { "type": "linear" }, "showPoints": "never", "spanNulls": true,
                "stacking": { "group": "A", "mode": "none" }, "thresholdsStyle": { "mode": "off" }
              },
              "mappings": [],
              "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
              "unit": "reqps"
            },
            "overrides": []
          },
          "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
          "id": 12,
          "options": {
            "legend": { "calcs": ["last", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
            "tooltip": { "mode": "multi", "sort": "none" }
          },
          "pluginVersion": "10.0.0",
          "targets": [
            { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(auth_session_created_total[5m])) by (backend)", "legendFormat": "Created - {{backend}}", "refId": "A" },
            { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "sum(rate(auth_session_revoked_total[5m])) by (backend)", "legendFormat": "Revoked - {{backend}}", "refId": "B" }
          ],
          "title": "Session Lifecycle Operations",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "description": "Active sessions by backend storage type over time.",
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": {
                "axisCenteredZero": false, "axisColorMode": "text", "axisLabel": "", "axisPlacement": "auto",
                "barAlignment": 0, "drawStyle": "line", "fillOpacity": 10, "gradientMode": "none",
                "hideFrom": { "tooltip": false, "viz": false, "legend": false },
                "lineInterpolation": "linear", "lineWidth": 1, "pointSize": 5,
                "scaleDistribution": { "type": "linear" }, "showPoints": "never", "spanNulls": true,
                "stacking": { "group": "A", "mode": "normal" }, "thresholdsStyle": { "mode": "off" }
              },
