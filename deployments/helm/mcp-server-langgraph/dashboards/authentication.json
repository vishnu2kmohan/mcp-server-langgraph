      "tags": [],
      "targetBlank": false,
      "title": "OpenFGA",
      "tooltip": "View OpenFGA Authorization Dashboard",
      "type": "link",
      "url": "/d/openfga/openfga"
    },
    {
      "asDropdown": false,
