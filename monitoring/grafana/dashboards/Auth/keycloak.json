            { "datasource": { "type": "prometheus", "uid": "${datasource}" }, "expr": "system_cpu_usage{job=\"keycloak\"} or vendor_system_cpu_utilization{job=\"keycloak\"}", "legendFormat": "System CPU", "refId": "B" }
          ],
          "title": "CPU Usage",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "description": "Memory usage from JVM heap metrics.",
          "fieldConfig": {
