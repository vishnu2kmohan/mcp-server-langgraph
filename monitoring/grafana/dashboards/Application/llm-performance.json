        "textMode": "auto"
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "sum(rate(agent_calls_successful_total[5m])) + sum(rate(agent_calls_failed_total[5m]))",
          "refId": "A"
        }
      ],
      "title": "Request Rate",
      "type": "stat"
    },
